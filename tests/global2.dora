//= output "x = 1\ny = 2\nz = 3\n"

var x: Int;
var y: Int;
var z: Int;

fun main() {
    init_globals();
    assert_globals();
    dump_globals();
}

fun init_globals() {
    x = 1;
    y = 2;
    z = 3;
}

fun assert_globals() {
    assert(x == 1);
    assert(y == 2);
    assert(z == 3);
}

fun dump_globals() {
    println("x = " + x.toString());
    println("y = " + y.toString());
    println("z = " + z.toString());
}