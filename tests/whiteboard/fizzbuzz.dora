//= stdout file

fun main(): Unit {
    let mut i = 1i32;

    while i <= 100i32 {
        if i.remainder(15i32) == 0i32 {
            println("FizzBuzz");

        } else if i.remainder(5i32) == 0i32 {
            println("Buzz");

        } else if i.remainder(3i32) == 0i32 {
            println("Fizz");
        } else {
            println(i.toString());
        }

        i = i + 1i32;
    }
}
