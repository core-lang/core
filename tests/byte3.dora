fun main() {
    let ba = arrayEmpty[Byte]();
    assert(ba.length() == 0);

    let ba = arrayFill[Byte](10, 2Y);
    assert(ba.length() == 10);

    ba.set(0, 10Y);
    ba.set(1, 11Y);
    ba.set(2, 12Y);

    assert(10Y == ba.get(0));
    assert(11Y == ba.get(1));
    assert(12Y == ba.get(2));
    assert(2Y == ba.get(9));
}