//= output "abxy\n"

fun main() {
    let x = arrayEmpty[String]();
    assert(x.length() == 0);

    let x = arrayFill[String](10, nil);
    assert(x.length() == 10);

    x.set(0, "a" + "b");
    x.set(1, "x" + "y");

    forceCollect();

    print(x.get(0));
    println(x.get(1));

    assert(x.get(0) == "ab");
    assert(x.get(1) == "xy");
    assert(x.get(2) === nil);
    assert(x.get(9) === nil);
}