fun main() {
  let a = 1i32;
  let b = 2i32;

  // a + h() --> 4
  // b * i() --> 10
  // g(4, 10i32) --> 36
  // j() --> 7
  // f(36, 7i32) --> 288

  assert(f(g(a + h(), b * i()), j()) == 288i32);
}

fun h(): Int32 { return 3i32; }
fun i(): Int32 { return 5i32; }
fun j(): Int32 { return 7i32; }

fun f(x: Int32, y: Int32): Int32 {
  return x * (y+1i32);
}
fun g(x: Int32, y: Int32): Int32 {
  return x * (y-1i32);
}
