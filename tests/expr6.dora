fun main() {
  let a = 1;
  let b = 2;

  // a + h() --> 4
  // b * i() --> 10
  // g(4, 10) --> 36
  // j() --> 7
  // f(36, 7) --> 288

  assert(f(g(a + h(), b * i()), j()) == 288);
}

fun h() -> Int { return 3; }
fun i() -> Int { return 5; }
fun j() -> Int { return 7; }

fun f(x: Int, y: Int) -> Int {
  return x * (y+1);
}
fun g(x: Int, y: Int) -> Int {
  return x * (y-1);
}
