fun main() {
  let ok1 = Result[Int32, String]::Ok(23);
  let ok2 = Result[Int32, String]::Ok(42);
  let er1 = Result[Int32, String]::Err("err1");
  let er2 = Result[Int32, String]::Err("err2");

  assert(ok1.or(ok2).contains(23));
  assert(ok1.or(er1).contains(23));
  assert(er1.or(ok2).contains(42));
  assert(er1.or(er2).getErrOrPanic() == "err2");
}
