fun main(): Unit {
    let x = std::range(0i32, 5i32).makeIterator();

    assert(x.next().getOrPanic() == 0i32);
    assert(x.next().getOrPanic() == 1i32);
    assert(x.next().getOrPanic() == 2i32);
    assert(x.next().getOrPanic() == 3i32);
    assert(x.next().getOrPanic() == 4i32);
    x.next().unwrapNone();
}
