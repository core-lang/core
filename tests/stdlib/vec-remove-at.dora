fun main() {
    let x = Vec::<Int>();

    x.push(1);
    x.push(2);
    x.push(3);

    assert(3 == x.removeAt(2));
    assert(x[0] == 1);
    assert(x[1] == 2);
    assert(x.len() == 2);

    assert(1 == x.removeAt(0));
    assert(x[0] == 2);
    assert(x.len() == 1);

    assert(2 == x.removeAt(0));
    assert(x.len() == 0);
}