fun main() {
    assert(f[Bar]() == 1);
    assert(f[Baz]() == 2);
}

trait X {
    fun foo() -> Int32;
}

fun f[T: X]() -> Int32 {
    return T::foo();
}

class Bar

impl X for module Bar {
    fun foo() -> Int32 {
        return 1;
    }
}

class Baz

impl X for module Baz {
    fun foo() -> Int32 {
        return 2;
    }
}
