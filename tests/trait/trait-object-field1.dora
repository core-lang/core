trait Foo { fun test(): Int64; }

class Baz

impl Foo for Baz {
    fun test(): Int64 {
        4711
    }
}

class FooBar

impl Foo for FooBar {
    fun test(): Int64 {
        42
    }
}

struct SomeData {
    foo: Foo,
}

class SomeClass(foo: Foo)

fun main(): Unit {
    let data = SomeData(Baz() as Foo);
    let foo = data.foo;
    assert(foo.test() == 4711);

    let data = SomeClass(Baz() as Foo);
    let foo = data.foo;
    assert(foo.test() == 4711);

    data.foo = FooBar() as Foo;
    let foo = data.foo;
    assert(foo.test() == 42);
}
