fun main(): Unit {
  let x = "hello";
  assert(x === x);
  assert(x == "hello");
  assert("a" == "a");
  assert("a" != "b");

  assert("a" < "b");
  assert("b" > "a");

  assert("a" <= "b");
  assert("b" <= "b");

  assert("b" >= "b");
  assert("b" >= "a");

  let y = "hel" + "lo";
  assert(x == y);

  let z = "h".plus("ello");
  assert(y == z);

  assert("a".compareTo("b") < 0i32);
  assert("b".compareTo("b") == 0i32);
  assert("b".compareTo("a") > 0i32);

  assert("a".equals("b").not);
  assert("a".equals("a"));
}
