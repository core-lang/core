fn main() {
    let nan = 0.0 / 0.0;
    assert((nan < 0.0).not());
    assert((nan > 0.0).not());
    assert((nan <= 0.0).not());
    assert((nan >= 0.0).not());
    assert((nan == 0.0).not());
    assert(nan != 0.0);

    assert((nan < nan).not());
    assert((nan > nan).not());
    assert((nan <= nan).not());
    assert((nan >= nan).not());
    assert((nan == nan).not());
    assert(nan != nan);

    let infp = 1.0 / 0.0;
    assert((nan < infp).not());
    assert((nan > infp).not());
    assert((nan <= infp).not());
    assert((nan >= infp).not());
    assert((nan == infp).not());
    assert(nan != infp);

    let infn = -1.0 / 0.0;
    assert((nan < infn).not());
    assert((nan > infn).not());
    assert((nan <= infn).not());
    assert((nan >= infn).not());
    assert((nan == infn).not());
    assert(nan != infn);
}
