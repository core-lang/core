fun main(): Unit {
    assert(tpl1() == 1i64);
    assert(tpl2() == 2i64);
    assert(tpl3() == 3i64);
    assert(tplsum() == 6i64);
}

fun tpl1(): Int64 {
    let x = (1i64, 2i64, 3i64);
    return x.0;
}

fun tpl2(): Int64 {
    let x = (1i64, 2i64, 3i64);
    return x.1;
}

fun tpl3(): Int64 {
    let x = (1i64, 2i64, 3i64);
    return x.2;
}

fun tplsum(): Int64 {
    let x = (1i64, 2i64, 3i64);
    return x.0 + x.1 + x.2;
}
