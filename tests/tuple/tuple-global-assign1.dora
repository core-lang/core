let mut x: (String, Bool) = ("", false);
let mut y: (String, Bool) = ("", false);
let mut tmp: (String, Bool) = ("", false);

fun main() {
    x = ("my string", true);
    y = ("another string", false);
    tmp = x;

    x = y;
    y = tmp;

    assert(y.0 == "my string");
    assert(y.1);

    assert(x.0 == "another string");
    assert(x.1.not());
}
