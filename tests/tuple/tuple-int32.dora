fun main(): Unit {
    assert(tpl1() == 1i32);
    assert(tpl2() == 2i32);
    assert(tpl3() == 3i32);
    assert(tplsum() == 6i32);
}

fun tpl1(): Int32 {
    let x = (1i32, 2i32, 3i32);
    return x.0;
}

fun tpl2(): Int32 {
    let x = (1i32, 2i32, 3i32);
    return x.1;
}

fun tpl3(): Int32 {
    let x = (1i32, 2i32, 3i32);
    return x.2;
}

fun tplsum(): Int32 {
    let x = (1i32, 2i32, 3i32);
    return x.0 + x.1 + x.2;
}
