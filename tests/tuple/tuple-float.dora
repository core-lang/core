fun main(): Unit {
    assert(tpl1() == 1.0f32);
    assert(tpl2() == 2.0f32);
    assert(tpl3() == 3.0f32);
    assert(tplsum() == 6.0f32);
}

fun tpl1(): Float32 {
    let x = (1.0f32, 2.0f32, 3.0f32);
    return x.0;
}

fun tpl2(): Float32 {
    let x = (1.0f32, 2.0f32, 3.0f32);
    return x.1;
}

fun tpl3(): Float32 {
    let x = (1.0f32, 2.0f32, 3.0f32);
    return x.2;
}

fun tplsum(): Float32 {
    let x = (1.0f32, 2.0f32, 3.0f32);
    return x.0 + x.1 + x.2;
}
