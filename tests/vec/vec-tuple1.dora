//= vm-args "--gc-verify --gc=copy"

fun main() {
    let x = build_vec();
    std::forceCollect();
    assert(x.size() == 3i64);
    pop_vec(x, "ghi");
    std::forceCollect();
    pop_vec(x, "def");
    std::forceCollect();
    pop_vec(x, "abc");
    std::forceCollect();
    assert(x.size() == 0i64);
}

fun build_vec(): Vec[(Int32, String)] {
    Vec[(Int32, String)]::new((1i32, "a" + "b" + "c"), (2i32, "d" + "e" + "f"), (3i32, "g" + "h" + "i"))
}

fun pop_vec(vec: Vec[(Int32, String)], expected: String) {
    let (_, str) = vec.pop().getOrPanic();
    assert(str == expected);
}
