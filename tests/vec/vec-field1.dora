class Foo {
    vec: Vec[Int32],
}

fun main(): Unit {
    let foo = Foo(Vec[Int32]::new(1i32, 2i32, 3i32));
    assert((foo.vec)(0i64) == 1i32);
    assert((foo.vec)(1i64) == 2i32);
    assert((foo.vec)(2i64) == 3i32);
    assert(foo.vec.size() == 3i64);
    foo.vec(0i64) = 100i32;
    assert((foo.vec)(0i64) == 100i32);
    assert((foo.vec)(1i64) == 2i32);
    assert((foo.vec)(2i64) == 3i32);
}
