fun main() {
    assert(2.0f32.compareTo(4.0f32) == -1i32);
    assert(4.0f32.compareTo(2.0f32) == 1i32);
    assert(4.0f32.compareTo(4.0f32) == 0i32);

    assert(2.0f32.compareTo(-4.0f32) == 1i32);
    assert((-4.0f32).compareTo(-2.0f32) == -1i32);
    assert((-4.0f32).compareTo(-4.0f32) == 0i32);

    assert(2.0.compareTo(4.0) == -1i32);
    assert(4.0.compareTo(2.0) == 1i32);
    assert(4.0.compareTo(4.0) == 0i32);

    assert(2.0.compareTo(-4.0) == 1i32);
    assert((-4.0).compareTo(-2.0) == -1i32);
    assert((-4.0).compareTo(-4.0) == 0i32);
}