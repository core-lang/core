//= vm-args "--gc-parallel-minor --gc-worker=2 --gc-verify"

fun main() {
    var x = Vec::<Array<Int> >();
    var i = 0;

    while i < 100 {
        if i % 2 == 0 {
            x.push(Array::<Int>(2));
        } else {
            x.push(Array::<Int>(4));
        }
        x.push(Array::<Int>(512));
        x.push(Array::<Int>(2048));
        if i % 5 == 0 {
            forceMinorCollect();
        }
        i = i + 1;
    }
}

