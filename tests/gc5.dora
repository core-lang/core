fun main() {
  let a = nested();
  std::forceCollect();

  assert(a.next.getOrPanic().next.getOrPanic().value == 3);
}

fun nested(): Foo {
  let x = Foo(3, None[Foo]);
  let y = Foo(2, Some[Foo](x));
  let z = Foo(1, Some[Foo](y));

  return z;
}

class Foo(let value: Int32, let next: Option[Foo])
