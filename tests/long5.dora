fun main() {
    let ba = Array[Long](0);
    assert(ba.length() == 0);

    let ba = arrayFill[Long](10, 2L);
    assert(ba.length() == 10);

    ba.set(0, 10L);
    ba.set(1, 11L);
    ba.set(2, 12L);

    assert(10L == ba.get(0));
    assert(11L == ba.get(1));
    assert(12L == ba.get(2));
    assert(2L == ba.get(9));
}