fun main() {
    let array = arrayEmpty::<Float>();
    assert(0 == array.len());

    let array = arrayFill::<Float>(10, 2F);
    assert(10 == array.len());
    assert(2F == array[0]);
    assert(2F == array[9]);

    let array = arrayEmpty::<Double>();
    assert(0 == array.len());

    let array = arrayFill::<Double>(12, 3D);
    assert(12 == array.len());
    assert(3D == array[0]);
    assert(3D == array[11]);
}