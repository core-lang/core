fun main(): Unit {
  let f = Foo(12i32);
  let mut a1 = f.a1();
  let a2 = f.a2();
  assert(a1 == a2);

  f.setA(101i32);
  a1 = f.a1();
  assert(101i32 == a1);
}

class Foo(a: Int32)

impl Foo {
  fun a1(): Int32 { return self.a; }
  fun a2(): Int32 { return self.a; }

  fun setA(a: Int32): Unit { self.a = a; }
}
