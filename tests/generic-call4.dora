//= stdout "world\n"

trait Foo {
    fun bar(): Int32;
}

class A[T: Foo](t: T)

impl[T: Foo] A[T] {
    fun bar(): Int32 {
        return self.t.bar();
    }
}

class X

impl Foo for X {
    fun bar(): Int32 {
        println("world");
        return 2i32;
    }
}

fun main() {
    assert(2i32 == A[X](X()).bar());
}
