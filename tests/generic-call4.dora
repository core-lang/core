//= output "world\n"

trait Foo {
    fun bar() -> Int;
}

class A<T: Foo>(let t: T) {
    fun bar() -> Int {
        return self.t.bar();
    }
}

class X

impl Foo for X {
    fun bar() -> Int {
        println("world");
        return 2;
    }
}

fun main() {
    assert(2 == A::<X>(X()).bar());
}