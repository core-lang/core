fun main() {
    let x = A::default();
    //let x = A::foo(); // crash
    assert(x === nil);
}

trait DefaultValue {
    fun default() -> Self;
}

class A(let a: Int32) {
  fun foo() -> Int32 = self.a;
}

module A {}

impl DefaultValue for module A {
    fun default() -> A {
        return nil;
    }
}