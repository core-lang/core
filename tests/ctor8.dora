fun main() {
  let x = X(1);

  assert(x.a == 1);
  assert(x.b == 0);
  assert(x.c == 0);

  let x = X(1, 2);

  assert(x.a == 1);
  assert(x.b == 2);
  assert(x.c == 0);

  let x = X(1, 2, 3);

  assert(x.a == 1);
  assert(x.b == 2);
  assert(x.c == 3);
}

class X(let a: Int, let b: Int, let c: Int) {
  init(a: Int) : self(a, 0, 0) {}
  init(a: Int, b: Int) : self(a, b, 0) {}
}
