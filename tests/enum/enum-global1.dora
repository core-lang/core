//= vm-args "--gc=copy"

enum Foo { A(Int32, String, Float32), B }

let mut foo_value: Foo = Foo::B;

fun main() {
    match foo_value {
        Foo::A(_, _, _) => unreachable[()](),
        Foo::B => (),
    }

    foo_value = Foo::A(112i32, "abc", 17.5f32);
    std::forceCollect();

    match foo_value {
        Foo::A(a, b, c) => {
            assert(a == 112i32);
            assert(b == "abc");
            assert(c == 17.5f32);
        },

        Foo::B => unreachable[()](),
    }
}
