enum Foo { A, B }

fun main(): Unit {
    assert(is_a(Foo::A));
    assert(is_a(Foo::B).not);
    assert(Foo::A == myid(Foo::A));
    assert(Foo::B == myid(Foo::B));
    assert(Foo::A != myid(Foo::B));
    assert(Foo::B != myid(Foo::A));
}

fun is_a(foo: Foo): Bool {
    return foo == Foo::A;
}

fun myid(foo: Foo): Foo {
    return foo;
}
