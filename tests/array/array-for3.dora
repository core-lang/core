fun main(): Unit {
    assert(mysum(Array[Int32]::new()) == 0i32);
    assert(mysum(Array[Int32]::new(1i32)) == 1i32);
    assert(mysum(Array[Int32]::new(1i32, 2i32, 3i32, 4i32, 5i32)) == 15i32);
}

fun mysum(x: Array[Int32]): Int32 {
    let mut entries = 0i32;
    for val in x {
        entries = entries + val;
    }
    entries
}
