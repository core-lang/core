fun main(): Unit {
    let array = Array[Bool]::zero(10i64);
    assert(array(0i64) == false);
    assert(array(9i64) == false);

    let array = Array[UInt8]::zero(10i64);
    assert(array(0i64) == 0u8);
    assert(array(9i64) == 0u8);

    let array = Array[Char]::zero(10i64);
    assert(array(0i64) == '\0');
    assert(array(9i64) == '\0');

    let array = Array[Int32]::zero(10i64);
    assert(array(0i64) == 0i32);
    assert(array(9i64) == 0i32);

    let array = Array[Int64]::zero(10i64);
    assert(array(0i64) == 0i64);
    assert(array(9i64) == 0i64);

    let array = Array[Float32]::zero(10i64);
    assert(array(0i64) == 0.0f32);
    assert(array(9i64) == 0.0f32);

    let array = Array[Float64]::zero(10i64);
    assert(array(0i64) == 0.0);
    assert(array(9i64) == 0.0);
}
