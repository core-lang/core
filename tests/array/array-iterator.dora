fun main(): Unit {
  iterateManually();
  iterateWithForIn();
}

fun iterateManually(): Unit {
  let it = Array[String]::new("a", "b", "c").iterator();
  let value = it.next().getOrPanic();
  assert(value == "a");
  let value = it.next().getOrPanic();
  assert(value == "b");
  let value = it.next().getOrPanic();
  assert(value == "c");
  it.next().unwrapNone();
}

fun iterateWithForIn(): Unit {
  var idx = 0;
  for value in Array[String]::new("a", "b", "c").iterator() {
    if idx ... == 0i64 { assert(value == "a"); }
    ... == 1i64 { assert(value == "b"); }
    ... == 2i64 { assert(value == "c"); }
    else        { unreachable[()](); }
    idx = idx + 1;
  }
}
