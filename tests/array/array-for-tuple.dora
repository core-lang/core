fun main(): Unit {
    let array = Array[(Int32, String)]::new((1i32, "abc"), (2i32, "def"), (3i32, "ghi"));
    for (idx, name) in array {
        if idx == 1i32 {
            assert(name == "abc");
        } else if idx == 2i32 {
            assert(name == "def");
        } else if idx == 3i32 {
            assert(name == "ghi");
        } else {
            unreachable[()]();
        }
    }
}
