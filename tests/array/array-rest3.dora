fun main(): Unit {
    assert(sum_args(1.0f32, 2.0f32) == 3.0f32);
    assert(sum_args(7.0f32) == 7.0f32);
    assert(sum_args() == 0.0f32);
    assert(sum_args(1.0f32, 2.0f32, 3.0f32, 4.0f32, 5.0f32) == 15.0f32);
}

fun sum_args(x: Float32...): Float32 {
    let mut result = 0.0f32;
    let mut idx = 0i64;
    
    while idx < x.size() {
        result = result + x(idx);
        idx = idx + 1i64;
    }

    result
}
