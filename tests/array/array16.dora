// this should work after implementing the rule that x(y) is x.get(y) and x(y) = z is x.set(y, z)

class Foo(let x: Array[Int])

fun main() {
  let foo = Foo(arrayFill[Int](10, 123));
  let e = foo.x.get(2);        // replace with let e = foo.x(2);
  assert(e == 123);
  foo.x.set(3, 456); // replace with foo.x(3) = 456;
  assert(foo.x.get(3) == 456);
}
