fun main(): Unit {
    assert(f_depth1() == 20);
    assert(f_depth2() == 20);
    assert(f_depth3() == 20);
}

fun f_depth1(): Int64 {
    let mut x = 10;
    let f1 = ||: Unit { x = 20; };
    f1();
    x
}

fun f_depth2(): Int64 {
    let mut x = 10;
    let f1 = ||: (): () {
        let f2 = ||: Unit { x = 20; };
        f2
    };
    f1()();
    x
}

fun f_depth3(): Int64 {
    let mut x = 10;
    let f1 = ||: (): (): () {
        let f2 = ||: (): () {
            let f3 = ||: Unit { x = 20; };
            f3
        };

        f2
    };

    f1()()();

    x
}
