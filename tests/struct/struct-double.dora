fun main() {
    assert(tpl1() == 1.0);
    assert(tpl2() == 2.0);
    assert(tpl3() == 3.0);
    assert(tplsum() == 6.0);
}

fun tpl1(): Float64 {
    let x = Doubles(1.0, 2.0, 3.0);
    x.f1
}

fun tpl2(): Float64 {
    let x = Doubles(1.0, 2.0, 3.0);
    x.f2
}

fun tpl3(): Float64 {
    let x = Doubles(1.0, 2.0, 3.0);
    x.f3
}

fun tplsum(): Float64 {
    let x = Doubles(1.0, 2.0, 3.0);
    x.f1 + x.f2 + x.f3
}

struct Doubles(f1: Float64, f2: Float64, f3: Float64)
