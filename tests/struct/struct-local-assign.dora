fun main(): Unit {
    var x = Pair("my string", true);
    var y = Pair("another string", false);
    var tmp = x;
    x = y;
    y = tmp;
    assert(y.first == "my string");
    assert(y.second);
    assert(x.first == "another string");
    assert(x.second.not());
}

struct Pair(first: String, second: Bool)
