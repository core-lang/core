//= vm-args "--gc=copy --disable-tlab"

class TupleClass {
    value: Pair,
}

class Foo(value: Int32)

fun main(): Unit {
    let tpl = TupleClass(Pair(Foo(4711i32), 4712i64));
    std::forceCollect();
    assert(tpl.value.first.value == 4711i32);
    assert(tpl.value.second == 4712i64);
}

struct Pair(first: Foo, second: Int64)
