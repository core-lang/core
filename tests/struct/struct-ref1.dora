//= vm-args "--gc=copy --disable-tlab --gc-stress"

class Foo(value: Int32)

struct Bar(f1: Foo, f2: Foo, f3: Foo)

fun main(): Unit {
    assert(tpl1() == 1i32);
    assert(tpl2() == 2i32);
    assert(tpl3() == 3i32);
    assert(tplsum() == 6i32);
}

fun tpl1(): Int32 {
    let x = Bar(Foo(1i32), Foo(2i32), Foo(3i32));
    std::forceCollect();
    x.f1.value
}

fun tpl2(): Int32 {
    let x = Bar(Foo(1i32), Foo(2i32), Foo(3i32));
    std::forceCollect();
    x.f2.value
}

fun tpl3(): Int32 {
    let x = Bar(Foo(1i32), Foo(2i32), Foo(3i32));
    std::forceCollect();
    x.f3.value
}

fun tplsum(): Int32 {
    let x = Bar(Foo(1i32), Foo(2i32), Foo(3i32));
    std::forceCollect();
    x.f1.value + x.f2.value + x.f3.value
}
