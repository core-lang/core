fun main(): Unit {
    let threads = argv(0).toInt().getOrPanic();

    let mut i = 0;

    while i < threads {
        let thread = MyThread();
        thread.start();
        i = i + 1;
    }
}

class MyThread() extends Thread {
    @override fun run(): Unit {
        allocator();
    }
}

fun allocator(): Unit {
    let mut i = 0;

    while i < 20'000'000 {
        Foo();
        i = i + 1;
    }
}

class Foo
