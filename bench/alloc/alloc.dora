fun main() {
    let threads = argv(0).toInt().getOrPanic();

    let mut i = 0;

    while i < threads {
        let thread = MyThread();
        thread.start();
        i = i + 1;
    }
}

class MyThread() extends Thread {
    @override fun run() {
        allocator();
    }
}

fun allocator() {
    let mut i = 0;

    while i < 20_000_000 {
        Foo();
        i = i + 1;
    }
}

