class Interval {
    let number: Int;
    let width: Int;
    let height: Int;
    let yfrom: Int;
    let yto: Int;
    let total: Int;
    let threadid: Int;

    init(number: Int, width: Int, height: Int, yfrom: Int, yto: Int, total: Int, threadid: Int) {
        self.number = number;
        self.width = width;
        self.height = height;
        self.yfrom = yfrom;
        self.yto = yto;
        self.total = total;
        self.threadid = threadid;
    }
}