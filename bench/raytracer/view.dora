class View {
    var from: Vec3;
    var at: Vec3;
    var up: Vec3;
    var dist: Double;
    var angle: Double;
    var aspect: Double;

    init(from: Vec3, at: Vec3, up: Vec3, dist: Double, angle: Double, aspect: Double) {
        self.from = from;
        self.at = at;
        self.up = up;
        self.dist = dist;
        self.angle = angle;
        self.aspect = aspect;
    }
}